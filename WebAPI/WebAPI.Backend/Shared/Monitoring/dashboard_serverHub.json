{
  "id": null,
  "title": "Custom Metrics Dashboard",
  "tags": [],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "graph",
      "title": "Total Messages Size",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum(rate(TotalMessagesSize_bucket[5m])) by (le))",
          "interval": "",
          "legendFormat": "Total Messages Size",
          "refId": "A"
        }
      ]
    },
    {
      "type": "singlestat",
      "title": "Total Count Messages",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "TotalCountMessages",
          "refId": "B"
        }
      ]
    },
    {
      "type": "singlestat",
      "title": "Total Count Alarms",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "TotalCountAlarms",
          "refId": "C"
        }
      ]
    },
    {
      "type": "singlestat",
      "title": "Total Count Notifications",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "TotalCountNotifications",
          "refId": "D"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Latency",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum(rate(Latency_bucket[5m])) by (le))",
          "refId": "E"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Working Memory Usage",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "avg(WorkingMemoryUsage)",
          "refId": "F"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Private Memory Usage",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "avg(PrivateMemoryUsage)",
          "refId": "G"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Average Message Size",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "_averageMessageSize",
          "refId": "H"
        }
      ]
    }
  ]
}

